{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "outDir": "dist",
    "declaration": true,
    "emitDeclarationOnly": false,
    "declarationMap": false,
    "sourceMap": false,
    "stripInternal": true,
    "verbatimModuleSyntax": true
  },
  "include": ["src/tokens/colors.ts"]
}
